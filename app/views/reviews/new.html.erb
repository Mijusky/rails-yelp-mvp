<div class="container">
   <h1>Create Restaurant</h1>
   <%= simple_form_for [@restaurant, @review] do |f| %>
     <%= f.input :content %>
     <%= f.input :rating %>
     <%= f.submit "Add a review", class: "btn btn-success" %>
   <% end %>
</div>
<i class="fas fa-dog"></i><%= link_to "Home", restaurants_path %>
